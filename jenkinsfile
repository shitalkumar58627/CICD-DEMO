  
pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Project with jenkins!.........."

ps -aux >> report.rxt

}
}
stage('Test') 
{
steps{
echo "Testing the Project with jenkins!.........."
date 
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project with jenkins!.........."
}
}
}
}
